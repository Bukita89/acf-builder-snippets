	"addText": {
		"prefix": ["add-text"],
		"body": ["->addText('text_field', [",
					"['label' => '${1:label}',",
					"'instructions' => '${2:instructions}',",
					"'required' => 0,",
					"'wrapper' => ['width' => '','class' => '','id' => ''],",
					"'default_value' => '',",
					"'placeholder' => '',",
					"'prepend' => '',",
					"'append' => '',",
					"'maxlength' => ''])"],
